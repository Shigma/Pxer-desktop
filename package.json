{
  "name": "pxscope",
  "version": "0.3.0",
  "description": "A desktop app for Pixiv",
  "main": "main.prod.js",
  "scripts": {
    "build": "node build/build.js --init --tsc --hosts --icons",
    "build:init": "node build/build.js --init",
    "build:tsc": "node build/build.js --tsc",
    "build:hosts": "node build/build.js --hosts",
    "build:icons": "node build/build.js --icons",
    "bundle": "node build/bundle.js --init -css --dev",
    "bundle:css": "node build/bundle.js --css",
    "bundle:dev": "node build/bundle.js --dev",
    "bundle:prod": "node build/bundle.js --prod",
    "pack": "node build/pack.js",
    "pack:min": "node build/pack.js --min",
    "pack:only": "node build/pack.js --only",
    "start": "electron .",
    "start:dev": "electron main.dev.js",
    "start:prod": "electron main.prod.js",
    "transpile": "node build/transpile.js",
    "test": "node build/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shigma/pxscope.git"
  },
  "keywords": [
    "pixiv",
    "desktop",
    "downloader"
  ],
  "author": "Shigma",
  "license": "MIT",
  "devDependencies": {
    "@octokit/rest": "^15.12.0",
    "@sfc2js/clean-css": "^1.1.1",
    "@types/node": "^10.10.1",
    "@types/node-fetch": "^2.1.2",
    "@vue/devtools": "^1.1.0",
    "archiver": "^3.0.0",
    "clean-css": "^4.2.1",
    "electron": "^2.0.10",
    "electron-packager": "^12.1.2",
    "eslint": "^5.6.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "fast-deep-equal": "^2.0.1",
    "js-yaml": "^3.12.0",
    "sass": "^1.14.0",
    "semver": "^5.5.1",
    "sfc2js": "^3.3.2",
    "svg2ttf": "^4.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.1"
  },
  "dependencies": {
    "dns-packet": "^5.0.3",
    "element-ui": "^2.4.7",
    "neat-scroll": "^2.0.1",
    "node-fetch": "^2.2.0",
    "opn": "^5.4.0",
    "popper.js": "^1.14.4",
    "throttle-debounce": "^2.0.1",
    "vue": "^2.5.17",
    "vue-i18n": "^8.1.0",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  }
